<?xml version="1.0"?>
<configuration>
  <system.serviceModel>
    <extensions>
      <behaviorExtensions>
        <add name="authenticationMessage" type="Teleopti.Ccc.Sdk.Common.WcfExtensions.AuthenticationMessageBehaviorExtension, Teleopti.Ccc.Sdk.Common, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9e7932cdbb39c45f"/>
      </behaviorExtensions>
    </extensions>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_WithSession" openTimeout="00:02:00" sendTimeout="00:02:00" maxReceivedMessageSize="200000000" receiveTimeout="00:30:00">
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Ntlm"/>
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="EnableAuthentication">
          <dataContractSerializer maxItemsInObjectGraph="10000000"/>
          <authenticationMessage/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc" behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiCccLogOnService" name="ITeleoptiCccLogOnService"/>
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc" behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiOrganizationService" name="ITeleoptiOrganizationService"/>
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc" behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiSchedulingService" name="ITeleoptiSchedulingService"/>
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc" behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiForecastingService" name="ITeleoptiForecastingService"/>
    </client>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
