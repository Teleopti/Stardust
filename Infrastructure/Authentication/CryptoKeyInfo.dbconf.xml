<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Infrastructure"
                   namespace="Teleopti.Ccc.Infrastructure.Authentication"
									 schema="auth">
	<class name="CryptoKeyInfo" table="CryptoKeyStore">
		<id name="Id" access="nosetter.camelcase-underscore" type="guid" unsaved-value="null">
			<generator class="guid.comb" />
		</id>
		
		<property name="Bucket" />
