<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>

	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level: %message%newline" />
			</layout>
		</appender>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="..\..\..\Logs\TestRun.log" />
			<appendToFile value="true" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="3" />
			<maximumFileSize value="10MB" />
			<staticLogFileName value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="OFF" />
			<!-- set DEBUG to enable logging. Should be OFF in repo for performance-->
			<appender-ref ref="ConsoleAppender" />
		</root>
		<logger name="Teleopti.LogTime">
			<level value="ALL" />
			<appender-ref ref="RollingFileAppender" />
		</logger>
	</log4net>

	<connectionStrings>
		<add name="AnalyticsContext" connectionString="" providerName="System.Data.SqlClient" />
		<add name="Queue" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Wfm.Web.SqlQueue"/>
		<add name="Hangfire" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Wfm.Web.Hangfire"/>
		<add name="RtaApplication" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7);Application Name=Teleopti.Web.RTA.WebService"/>
		<add name="Tenancy" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7);Application Name=Teleopti.Tenancy"/>
		<add name="MessageBroker" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Web.Broker"/>
	</connectionStrings>

	<appSettings>
		<add key="configSanityCheck" value="Yes, configuration is loaded!" />
		<add key="datamartConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Analytics.EtlIntegrationTest" />
		<add key="cultures" value="en;ar;cs;da;de;es;fi;fr;it;ja;ru;pt;pl;sk-SK;sq-AL;sv;th;tr;zh-CN" />
		<add key="InfraTest.ConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7)"/>
		<add key="InfraTest.AnalyticsConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS)"/>
		<add key="InfraTest.SqlServerName" value="$(SQL_SERVER_NAME)"/>
		<add key="InfraTest.DatabaseName" value="$(DB_CCC7)"/>
	</appSettings>

	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.5.0.0" newVersion="3.5.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Web.Http" publicKeyToken="31bf3856ad364e35" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-5.2.2.0" newVersion="5.2.2.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

</configuration>
