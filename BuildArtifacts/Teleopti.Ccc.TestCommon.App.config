<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level: %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="OFF" /> <!-- set DEBUG to enable logging. Should be OFF in repo for performance-->
			<appender-ref ref="ConsoleAppender" />
		</root>
	</log4net>

  <connectionStrings>
    <add name="AnalyticsContext" connectionString=""
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="configSanityCheck" value="Yes, configuration is loaded!" />

	<add key="SitePath" value="..\..\..\Teleopti.Ccc.Web"/>	
	<add key="DB_ANALYTICS" value="$(DB_ANALYTICS)"/>
	<add key="DB_CCC7" value="$(DB_CCC7)"/>
	<add key="SQL_SERVER_NAME" value="$(SQL_SERVER_NAME)"/>
	<add key="SQL_LOGIN" value=""/>
	<add key="SQL_PASSWORD" value=""/>
	<add key="WEB_BROKER_BACKPLANE" value="$(WEB_BROKER_BACKPLANE)"/>
	<add key="AGENTPORTALWEB_nhibConfPath" value="$(AGENTPORTALWEB_nhibConfPath)"/>
	<add key="ToggleMode" value="$(TOGGLE_MODE)" />
  </appSettings>
</configuration>
