<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <!--WISEMETA: connectionString="[SQL_AUTH_STRING];Application Name=Teleopti.Ccc.ServiceBusHost.SqlQueue;Initial Catalog=[DB_ANALYTICS]"-->
    <add name="Queue" connectionString="Data Source=.;Integrated Security=True;Initial Catalog=$(AnalyticsDB)"/>
  </connectionStrings>
  <system.serviceModel>
    <extensions>
      <behaviorExtensions>
        <add name="authenticationMessage" type="Teleopti.Ccc.Sdk.Common.WcfExtensions.AuthenticationMessageBehaviorExtension, Teleopti.Ccc.Sdk.Common, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9e7932cdbb39c45f" />
      </behaviorExtensions>
    </extensions>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_WithSession" openTimeout="00:02:00"
          sendTimeout="00:02:00" maxReceivedMessageSize="500000000" receiveTimeout="05:30:00">
		  <!--WISEMETA: mode="[SDK_SSL_SECURITY_MODE]"-->
          <security mode="TransportCredentialOnly">
            <!--WISEMETA: clientCredentialType="[SDK_CRED_PROT]"-->
            <transport clientCredentialType="Ntlm" />
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="EnableAuthentication">
          <authenticationMessage />
          <dataContractSerializer maxItemsInObjectGraph="100000000"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <!--WISEMETA: address="[AGENT_SERVICE]"-->
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc"
        behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiCccSdkInternal" name="ITeleoptiCccSdkInternal" />
      <!--WISEMETA: address="[AGENT_SERVICE]"-->
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc"
        behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiCccLogOnService" name="ITeleoptiCccLogOnService" />
      <!--WISEMETA: address="[AGENT_SERVICE]"-->
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc"
        behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiOrganizationService" name="ITeleoptiOrganizationService" />
      <!--WISEMETA: address="[AGENT_SERVICE]"-->
      <endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc"
        behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
        bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiSchedulingService" name="ITeleoptiSchedulingService" />

    </client>
  </system.serviceModel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
	<!-- Need this because:
	Reference: Rhino.Queues
	1.4.1.0 by Rhino.ServiceBus.Autofac
	1.4.1.0 by Rhino.ServiceBus
	-->
      <dependentAssembly>
        <assemblyIdentity name="Rhino.Queues" publicKeyToken="0b3305902db7183f" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.4.2.0" newVersion="1.4.2.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>