	<system.serviceModel>
		<extensions>
			<behaviorExtensions>
				<add name="authenticationMessage" type="Teleopti.Ccc.Sdk.Common.WcfExtensions.AuthenticationMessageBehaviorExtension, Teleopti.Ccc.Sdk.Common, Culture=neutral, PublicKeyToken=9e7932cdbb39c45f" />
			</behaviorExtensions>
		</extensions>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_WithSession" openTimeout="00:02:00"
				  sendTimeout="00:02:00" maxReceivedMessageSize="500000000" receiveTimeout="05:30:00">
					<security mode="$(SDK_SSL_SECURITY_MODE)">
						<transport clientCredentialType="None" />
					</security>
				</binding>
			</basicHttpBinding>
		</bindings>
		<behaviors>
			<endpointBehaviors>
				<behavior name="EnableAuthentication">
					<authenticationMessage />
					<dataContractSerializer maxItemsInObjectGraph="100000000"/>
				</behavior>
			</endpointBehaviors>
		</behaviors>
		<client>
			<endpoint address="$(AGENT_SERVICE)"
			  behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
			  bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiCccSdkInternal" name="ITeleoptiCccSdkInternal" />
			<endpoint address="$(AGENT_SERVICE)"
			  behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
			  bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiCccLogOnService" name="ITeleoptiCccLogOnService" />
			<endpoint address="$(AGENT_SERVICE)"
			  behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
			  bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiOrganizationService" name="ITeleoptiOrganizationService" />
			<endpoint address="$(AGENT_SERVICE)"
			  behaviorConfiguration="EnableAuthentication" binding="basicHttpBinding"
			  bindingConfiguration="BasicHttpBinding_WithSession" contract="Teleopti.Ccc.Sdk.Common.Contracts.ITeleoptiSchedulingService" name="ITeleoptiSchedulingService" />
		</client>
	</system.serviceModel>
