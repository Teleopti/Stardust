<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<appSettings file="SettingsForTest.config" >
		<add key="InfraTest.ToggleMode" value="$(ToggleMode)" />
		<add key="InfraTest.ConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7)"/>
		<add key="InfraTest.AnalyticsConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS)"/>
		<add key="InfraTest.SqlServerName" value="$(SQL_SERVER_NAME)"/>
		<add key="InfraTest.SqlAuthString" value="$(SQL_AUTH_STRING)"/>
		<add key="InfraTest.DatabaseName" value="$(DB_CCC7)"/>
		<add key="InfraTest.AnalyticsDatabaseName" value="$(DB_ANALYTICS)"/>
		<add key="InfraTest.DatabaseBackupLocation" value="\\buildsrv01\PerfTests\"/>
	</appSettings>

	<log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level: %message%newline" />
			</layout>
		</appender>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="..\..\..\Logs\TestRun.log" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="ERROR" />
			<appender-ref ref="ConsoleAppender" />
			<appender-ref ref="RollingFileAppender" />
		</root>
		<logger name="Teleopti.TestLog">
			<level value="DEBUG" />
		</logger>
	</log4net>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
