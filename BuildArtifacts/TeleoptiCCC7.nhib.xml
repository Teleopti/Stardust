<?xml version="1.0" encoding="utf-8"?>
<datasource>
	<hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
		<session-factory name="Teleopti WFM">
			<property name="connection.connection_string">
				<!--WISEMETA: default="[SQL_AUTH_STRING];initial Catalog=[DB_CCC7];Current Language=us_english"-->
				$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7);Current Language=us_english
			</property>
			<property name="command_timeout">60</property>
		</session-factory>
	</hibernate-configuration>
	<matrix>
		<connectionString>
			<!--WISEMETA: default="[SQL_AUTH_STRING];Initial Catalog=[DB_ANALYTICS];Current Language=us_english"-->
			$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Current Language=us_english
		</connectionString>
	</matrix>
</datasource>
