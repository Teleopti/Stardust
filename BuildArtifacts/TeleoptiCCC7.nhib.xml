<?xml version="1.0" encoding="utf-8"?>
<datasource>
	<hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
		<session-factory name="$(DATASOURCE_NAME)">
			<!-- properties -->
			<property name="connection.connection_string">
        $(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7);Current Language=us_english
      </property>
			<property name="command_timeout">60</property>
		</session-factory>
	</hibernate-configuration>
	<matrix name="MatrixDatamartDemo">
		<connectionString>
      $(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Current Language=us_english
    </connectionString>
	</matrix>
	<authentication>
		<logonMode>mix</logonMode>
		<!--  win or mix -->
	</authentication>
</datasource>
