<?xml version="1.0" encoding="utf-8"?>
<configuration>
	
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<connectionStrings>
		<add name="Hangfire" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Wfm.Web.Hangfire"/>
		<add name="Tenancy" connectionString="$(SQL_AUTH_STRING);Initial Catalog=$(DB_CCC7);Application Name=Teleopti.Etl.Tenancy"/>
	</connectionStrings>
	<appSettings>
    		<add key="databaseTimeout" value="1200"/>
    		<add key="concurrentThreads" value="2"/>
    		<add key="chunkTimeSpan" value="2"/>
    		<add key="isolationLevel" value="ReadCommitted"/>
    		<add key="pmInstallation" value="$(PM_INSTALL)"/>
		<add key="datamartConnectionString" value="$(SQL_AUTH_STRING);Initial Catalog=$(DB_ANALYTICS);Application Name=Teleopti.Wfm.Etl.Service"/>
		<add key="MessageBroker" value="$(WEB_BROKER_FOR_WEB)"/>
		<add key="Cube" value="Provider=MSOLAP.3;Integrated Security=SSPI;Persist Security Info=True;Data Source=$(AS_SERVER_NAME);Initial Catalog=$(AS_DATABASE);Client Cache Size=25;Auto Synch Period=10000"/>
		<add key="FeatureToggle" value="$(URL)"/>
		<add key="ManagerLocation" value="$(URL)StardustDashboard/" />
	</appSettings>
  
	<log4net>
		<appender name="EventAppender" type="log4net.Appender.EventLogAppender" >
			<applicationName value="TeleoptiETLService" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
			</layout>
		</appender>
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="..\..\..\Logs\ETL.Service.log" />
			<rollingStyle value="Size" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="200KB" />
			<staticLogFileName value="true" />
			<appendToFile value="true" />
			<layout type="log4net.Layout.PatternLayout">
			<param name="ConversionPattern" value="%d [%t] %-5p %c %m%n" />
			</layout>
		</appender>
		<logger name="NHibernate.Engine.StatefulPersistenceContext.ProxyWarnLog">
			<level value="ERROR" />
		</logger>
		<root>
			<level value="WARN" />
			<appender-ref ref="EventAppender" />
			<appender-ref ref="RollingLogFileAppender" />
		</root>
	</log4net>

	<system.serviceModel>
		<bindings>
			<wsHttpBinding>
				<binding name="Etl_Pm_Https_Binding" sendTimeout="00:15:00" maxReceivedMessageSize="2147483646" maxBufferPoolSize="2147483646">
					<security mode="Transport">
						<transport clientCredentialType="Ntlm" />
					</security>
				</binding>
				<binding name="Etl_Pm_Http_Binding" sendTimeout="00:15:00" maxReceivedMessageSize="2147483646" maxBufferPoolSize="2147483646">
				</binding>				
			</wsHttpBinding>
		</bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="behave">
          <clientCredentials>
            <windows allowedImpersonationLevel="Impersonation"/>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <endpoint address="$(PM_SERVICE)"
                binding="wsHttpBinding"
                contract="Teleopti.Analytics.Etl.Interfaces.PerformanceManager.IPMService"
                bindingConfiguration="$(ETLPM_BINDING_NAME)"
                behaviorConfiguration="behave" />
    </client>
  </system.serviceModel>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.5.0.0" newVersion="3.5.0.0" />
			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="Castle.Core" publicKeyToken="407dd0808d44fbdc" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.3.0.0" newVersion="3.3.0.0" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>
