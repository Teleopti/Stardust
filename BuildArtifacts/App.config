	</log4net>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_WithSession"
						 allowCookies="true"
						 maxReceivedMessageSize="655360" />
			</basicHttpBinding>
		</bindings>
		<client>
			<endpoint address="http://localhost:1335/TeleoptiCccSdkService.svc"
					  binding="basicHttpBinding"
					  contract="Teleopti.Ccc.Sdk.WcfContract.ITeleoptiCccSdkService"
					  bindingConfiguration="BasicHttpBinding_WithSession"
				>
			</endpoint>
		</client>
	</system.serviceModel>

	<appSettings>
		<add key="ClientSettingsProvider.ServiceUri" value=""/>
		<!--WISEMETA: value="[MATRIX_WEB_SITE_URL]"-->
		<add key="MatrixWebSiteUrl" value="http://localhost/TeleoptiWFM/Analytics/"/>
		<!--WISEMETA: value="[SQL_AUTH_STRING];Initial Catalog=[DB_MESSAGING]"-->
		<add key="MessageBroker" value="Data Source=.;Integrated Security=True;Initial Catalog=TeleoptiAnalytics;Persist Security Info=False;"/>
		<!--WISEMETA: value="[CONTEXT_HELP_URL]"-->
		<add key="HelpUrl" value="http://localhost/TeleoptiWFM/ContextHelp/"/>
		<!--WISEMETA: value="f01_"-->
		<add key="HelpPrefix" value="f01:"/>
		<!--WISEMETA: value="_"-->
		<add key="HelpDivider" value="+"/>
		<!--WISEMETA: value=".html"-->
		<add key="HelpSuffix" value=""/>
	</appSettings>
</configuration>