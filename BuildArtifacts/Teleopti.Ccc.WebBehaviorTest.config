<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow"/>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<specFlow>
		<runtime missingOrPendingStepsOutcome="Error" />
	</specFlow>
	<log4net>
		<root>
			<level value="OFF" /> 
		</root>
	</log4net>
  <appSettings>
    <add key="configSanityCheck" value="Yes, configuration is loaded!" />

	<add key="SitePath" value="..\..\..\Teleopti.Ccc.Web"/>	
	<add key="DB_ANALYTICS" value="$(DB_ANALYTICS)"/>
	<add key="DB_CCC7" value="$(DB_CCC7)"/>
	<add key="SQL_SERVER_NAME" value="$(SQL_SERVER_NAME)"/>
	<add key="WEB_BROKER_BACKPLANE" value="$(WEB_BROKER_BACKPLANE)"/>
	<add key="AGENTPORTALWEB_nhibConfPath" value="$(AGENTPORTALWEB_nhibConfPath)"/>
	<add key="ToggleMode" value="$(TOGGLE_MODE)" />
  </appSettings>
</configuration>
