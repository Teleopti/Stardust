<?xml version="1.0" encoding="utf-8" ?>
<Format xmlns="urn:payroll-format-1.0">
  <Document DocumentElement="DocumentElement" Separator="," Type="Csv">
    <Items Element="PayrollExport">
			<Item Element="FirstName" Type="string" />
			<Item Element="LastName" Type="string" />
			<Item Element="BusinessUnitName" Type="string" />
			<Item Element="SiteName" Type="string" />
			<Item Element="TeamName" Type="string" />
			<Item Element="ContractName" Type="string" />
			<Item Element="PartTimePercentageName" Type="string" />
			<Item Element="PartTimePercentageNumber" Type="decimal" />
			<Item Element="Date" Type="date" Format="YYYY-MM-DD" />
			<Item Element="PayrollCode" Type="string" />
			<Item Element="Time" Type="time" Format="hh:mm" />
    </Items>
  </Document>
</Format> 