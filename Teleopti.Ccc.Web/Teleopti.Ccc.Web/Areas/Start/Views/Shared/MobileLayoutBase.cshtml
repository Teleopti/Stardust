@{ Layout = null; }
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; minimum-scale=1.0; user-scalable=0;" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<title>@ViewBag.Title</title>
	<link rel="stylesheet" href="@Url.Content("~/Content/themes/mobile/mobile.min.css")" />
	<link rel="stylesheet" href="@Url.Content("~/Content/Mobile/jquery.mobile.structure-1.2.0.min.css")" />
	@RenderSection("csshead", required: false)
	<script>
		if (!window.console) { window.console = { log: function () { } }}
	</script>
	<script src="@Url.Content("~/Content/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Content/Scripts/json2.js")"></script>
	<script src="@Url.Content("~/Content/Scripts/date.js")"></script>
	@RenderSection("jsBeforeMobile", required: false)
	<script src="@Url.Content("~/Content/Mobile/jquery.mobile-1.2.0.min.js")" type="text/javascript"></script>
	@RenderSection("jsAfterMobile", required: false)
</head>
	<body>
		@RenderBody()
	</body>
</html>
