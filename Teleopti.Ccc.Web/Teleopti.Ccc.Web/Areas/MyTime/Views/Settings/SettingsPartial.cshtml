@using Teleopti.Ccc.Web.Areas.MyTime.Models.Settings
@model SettingsViewModel
@{
	 Layout = null;
}
Culture...<select>@foreach(var c in Model.Cultures)
						{
							<option value="@c.LCID" @if(c.LCID == Model.ChoosenCulture.LCID)
															{
<text>selected='selected'</text>
															}>@c.DisplayName</option>
						}</select>
<br/>
UiCulture...<select>@foreach(var c in Model.Cultures)
						{
							<option value="@c.LCID" @if(c.LCID == Model.ChoosenUiCulture.LCID)
															{
<text>selected='selected'</text>
															}>@c.DisplayName</option>
						}</select>
