{
	"masterPicture": "src/assets/favicon/wfm.svg",
	"iconsPath": "dist/ng2/assets/favicon/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#ffffff",
			"margin": "14%",
			"assets": {
				"ios6AndPriorIcons": false,
				"ios7AndLaterIcons": false,
				"precomposedIcons": false,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Teleopti WFM"
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "whiteSilhouette",
			"backgroundColor": "#0099ff",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": false,
					"medium": true,
					"big": false,
					"rectangle": false
				}
			},
			"appName": "Teleopti WFM"
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "17%",
			"backgroundColor": "#ffffff",
			"themeColor": "#ffffff",
			"manifest": {
				"name": "Teleopti WFM",
				"display": "browser",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#0099ff"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	},
	"versioning": {
		"paramName": "v",
		"paramValue": "2"
	}
