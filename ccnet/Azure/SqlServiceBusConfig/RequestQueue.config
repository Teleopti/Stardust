<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="rhino.esb" type="Rhino.ServiceBus.Config.BusConfigurationSection, Rhino.ServiceBus"/>
  </configSections>
  <rhino.esb>
    <bus threadCount="1"
				numberOfRetries="5"
				endpoint="sql.queues://$(BASEURL):2202/request"
				name="request"
				connectionString="$(ANALYTICS_CONNECTION)"
				queueIsolationLevel="ReadCommitted"/>
    <messages>
      <add name="Teleopti.Interfaces.Messages.Requests"
					 endpoint="sql.queues://$(BASEURL):2202/request"/>
      <add name="Teleopti.Interfaces.Messages.Denormalize"
        endpoint="sql.queues://$(BASEURL):2204/denormalize"/>
      <add name="Teleopti.Interfaces.Messages.General"
         endpoint="sql.queues://$(BASEURL):2203/general"/>
    </messages>
    <security>
      <key>f/gdDWbDqHRvpqdRbTs3mxhGdZh9qCaDrasxJGXl+5s=</key>
    </security>
  </rhino.esb>
</configuration>
