<?xml version="1.0" encoding="utf-8"?>

<Project DefaultTargets="CreateAzure" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<Target Name="CreateAzure">
		<Message Text="CCCVersion is: $(CCCVersion)" Importance="high" />

		<Message Text="Building Azure packages ..." Importance="high" />
		<Message Text="&quot;$(CallingCCNetWorkingDirectory)\ccnet\Azure\BuildAzurePackages.bat&quot; $(CCCVersion)"/>
		<Exec Command="&quot;$(CallingCCNetWorkingDirectory)\ccnet\Azure\BuildAzurePackages.bat&quot; $(CCCVersion)"/>
		<Message Text="Building Azure packages . Done!" Importance="high" />
	</Target>

</Project>