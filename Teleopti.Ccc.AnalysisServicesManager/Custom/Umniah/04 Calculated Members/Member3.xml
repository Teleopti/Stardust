<!--
This is where you define your calculated members.
The tag:
<AssociatedMeasureGroupID>
Sets what Measure Group you'll like the member to show up in.

Please note that the value of <CalculationReference> tag:
<CalculationReference>[Measures].[ThisIs My first Calculated Member]</CalculationReference>
must match the name of our created member:
<MdxString>CREATE MEMBER CURRENTCUBE.[Measures].[ThisIs My first Calculated Member]  .....
</MdxString>

-->
<CalculatedMemberDefinition>
	<CalculatedMember>
		<CalculationReference>[Measures].[CSAT Survey Score AVG]</CalculationReference>
		<AssociatedMeasureGroupID>Fact Agent Customer Survey</AssociatedMeasureGroupID>
		<MdxString>
		CREATE MEMBER CURRENTCUBE.[Measures].[CSAT Survey Score AVG]  AS [Measures].[Score]/[Measures].[CSAT Number Of Calls], FORMAT_STRING = "0", VISIBLE = 1  ;
		</MdxString>
	</CalculatedMember>
	<CalculatedMember>
		<CalculationReference>[Measures].[QA Quality Score AVG]</CalculationReference>
		<AssociatedMeasureGroupID>Fact Agent Evaluation</AssociatedMeasureGroupID>
		<MdxString>
		CREATE MEMBER CURRENTCUBE.[Measures].[QA Quality Score AVG]  AS ([Measures].[Quality Score Sum]/100)/[Measures].[QA Number Of Evaluations], FORMAT_STRING = "0%", VISIBLE = 1  ;
		</MdxString>
	</CalculatedMember>
	
</CalculatedMemberDefinition>