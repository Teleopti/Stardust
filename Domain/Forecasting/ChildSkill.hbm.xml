<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Domain"
                   namespace="Teleopti.Ccc.Domain.Forecasting"
                   default-access="field.camelcase-underscore">
  <joined-subclass name="ChildSkill"
					extends="Skill">
    <key column="Skill" foreign-key="FK_ChildSkill_Skill"/>
    <many-to-one class="MultisiteSkill"
                 name="ParentSkill"
                 not-null="false" foreign-key="FK_ChildSkill_MultisiteSkill"/>
  </joined-subclass>
</hibernate-mapping>
