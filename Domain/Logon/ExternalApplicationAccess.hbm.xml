<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Domain"
                   namespace="Teleopti.Ccc.Domain.Logon"
                   default-access="field.camelcase-underscore"
				   schema="Tenant">
  <class name="ExternalApplicationAccess">
    <!--IEntity-->
    <id name="Id"
				type="guid"
		  access="nosetter.camelcase-underscore"
				unsaved-value="null">
      <generator class="guid.comb" />
    </id>
    <!--IEntity-->
	<property name="CreatedOn" type="UtcDateTime" access="property" />
	<property name="Hash" type="string" length="512" access="property" />
	<property name="Name" type="string" length="256" access="property" />
	<property name="PersonId" type="Guid" access="property" />

  </class>
</hibernate-mapping>

