<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Domain"
                   namespace="Teleopti.Ccc.Domain.Payroll"
                   default-access="field.camelcase-underscore">
  <class name="XmlResult">
    <!--IEntity-->
    <id name="Id"
      type="guid"
		  access="nosetter.camelcase-underscore"
      unsaved-value="null">
      <generator class="guid.comb" />
    </id>

    <!--IEntity-->

    <property name="XPathNavigable"
          not-null="false"
               type="string" length="1000000" />

    <one-to-one name ="Parent"
            class="PayrollResult"
            property-ref="XmlResult"
            foreign-key="FK_XmlResult_PayrollResult"/>
  </class>
</hibernate-mapping>

