<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="Teleopti.Ccc.Domain"
				   namespace="Teleopti.Ccc.Domain.WorkflowControl"
				   default-access="field.camelcase-underscore">
	<class name="OvertimeRequestOpenPeriodSkillType">
		<!--IEntity-->
		<id name="Id"
			type="guid"
			access="nosetter.camelcase-underscore"
			unsaved-value="null">
			<generator class="guid.comb" />
		</id>
		<!--IEntity-->
		<many-to-one name="Parent"
					 class="OvertimeRequestOpenPeriod"
					 not-null ="true"
					 foreign-key="FK_OvertimeRequestOpenPeriodSkillType_OvertimeRequestOpenPeriod"/>
		<many-to-one name="SkillType"
					 class="Teleopti.Ccc.Domain.Forecasting.SkillType"
					 lazy="false"
					 foreign-key="FK_OvertimeRequestOpenPeriodSkillType_SkillType"/>
	</class>
</hibernate-mapping>
