<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="Teleopti.Ccc.Domain" 
                   namespace="Teleopti.Ccc.Domain.WorkflowControl"
                   default-access="field.camelcase-underscore">
  <subclass name="OvertimeRequestOpenRollingPeriod"
            extends="OvertimeRequestOpenPeriod"
            discriminator-value="RollingPeriod">

    <component class ="Teleopti.Interfaces.Domain.MinMax`1[System.Int32], Teleopti.Interfaces" name="BetweenDays">
      <property name="Minimum" column="DaysMinimum" not-null="false" type="int"/>
      <property name="Maximum" column="DaysMaximum" not-null="false" type="int"/>
    </component>
  </subclass>
         
</hibernate-mapping>
