<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Domain"
                   namespace="Teleopti.Ccc.Domain.Analytics"
									 schema="mart">
	<class name="AnalyticsPermission" table="permission_report">
		<composite-id>
			<key-property name="PersonCode" column="person_code"/>
			<key-property name="TeamId" column="team_id"/>
			<key-property name="MyOwn" column="my_own"/>
			<key-property name="BusinessUnitId" column="business_unit_id"/>
			<key-property name="ReportId" column="ReportId"/>
		</composite-id>
		<property name="DatasourceId" column="datasource_id"/>
		<property name="DatasourceUpdateDate" column="datasource_update_date"/>
	</class>
</hibernate-mapping>