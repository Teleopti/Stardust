<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Ccc.Domain"
                   namespace="Teleopti.Ccc.Domain.RealTimeAdherence"
                   default-access="field.camelcase-underscore">

	<class name="RtaState">
		<id name="Id" type="guid" access="nosetter.camelcase-underscore" unsaved-value="null">
			<generator class="guid.comb" />
		</id>
		<many-to-one name="BusinessUnit" class="Teleopti.Ccc.Domain.Common.BusinessUnit" access="readonly" />
		<property name="Name" />
		<property name="StateCode" />
		<property name="PlatformTypeId" />
		<many-to-one class="RtaStateGroup" name="Parent" />
	</class>
</hibernate-mapping>
