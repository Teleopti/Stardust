<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Teleopti.Ccc.Domain" namespace="Teleopti.Ccc.Domain.Common" default-access="field.camelcase-underscore">
	<class name="ExternalPerformanceData">
		<id name="Id" access="nosetter.camelcase-underscore" type="guid" unsaved-value="null">
			<generator class="guid.comb" />
		</id>

		<many-to-one name="ExternalPerformance" class="Teleopti.Ccc.Domain.Common.ExternalPerformance" foreign-key="FK_ExternalPerformanceData_ExternalPerformance" not-null="true" />
		<property name="DateFrom" type="UtcDateTime" not-null ="true" />
		<property name="PersonId" type="guid" not-null="true" />
		<property name="OriginalPersonId" type="string" length="130" not-null="true" />
		<property name="Score" type="double" not-null="true" />
	</class>
</hibernate-mapping>