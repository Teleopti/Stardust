<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Teleopti.Interfaces"
                   namespace="Teleopti.Interfaces.MessageBroker">
	<class name="Notification" table="msg.Notification">
		<id name="Id" type="guid">
			<generator class="guid.comb" />
		</id>
		<property name="DataSource"/>
		<property name="BusinessUnitId"/>
		<property name="DomainType"/>
		<property name="DomainQualifiedType"/>
		<property name="DomainId"/>
		<property name="ModuleId"/>
		<property name="DomainReferenceId"/>
		<property name="DomainReferenceType"/>
		<property name="StartDate" type="Teleopti.Ccc.Infrastructure.NHibernateConfiguration.StringSavedAsDateTime, Teleopti.Ccc.Infrastructure"/>
		<property name="EndDate" type="Teleopti.Ccc.Infrastructure.NHibernateConfiguration.StringSavedAsDateTime, Teleopti.Ccc.Infrastructure"/>
		<property name="DomainUpdateType"/>
		<property name="BinaryData"/>
		<many-to-one name="MailboxParent" class="Mailbox" />
	</class>
</hibernate-mapping>